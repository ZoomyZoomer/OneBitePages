{
    "buildCommand": "cd blogsite && npm install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "blogsite/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        }
    ],
    "builds": [
        {
          "src": "api/index.js",
          "use": "@vercel/node",
          "config": { "maxDuration": 300 }
        }
      ]
}