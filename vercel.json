{
    "buildCommand": "cd blogsite && npm install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "blogsite/dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
          "source": "/(.*)",
          "destination": "/blogsite/dist/$1"
        }
    ]
}